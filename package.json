    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-template-literals": "^7.27.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "~19.1.0",
    "@vitest/coverage-v8": "^4.0.13",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "expo-mcp": "~0.2.1",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "madge": "^8.0.0",
    "supabase": "^2.54.11",
    "typescript": "~5.9.2",
    "vite-tsconfig-paths": "^5.1.1",
    "vitest": "^4.0.13",
    "workbox-cli": "^7.4.0"
  },
  "private": true
}
