{
  "name": "dream-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:mock": "cp .env.mock .env.local && expo start",
    "start:real": "rm -f .env.local && expo start",
    "start:supabase": "node ./scripts/start-supabase.js",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "npx expo export -p web",
    "lint": "expo lint",
    "test": "vitest",
    "test:e2e": "maestro test maestro/mock-existing-user.yml",
    "test:e2e:mock-quotas": "maestro test maestro/mock-existing-quotas.yml",
    "test:e2e:quotas": "maestro test maestro/guest-quota-flow.yml",
    "test:e2e:guest-limit": "maestro test maestro/guest-dream-limit.yml",
    "build:apk": "echo 'Usage: npm run build:apk:mock | npm run build:apk:prod'",
    "build:apk:mock": "EXPO_PUBLIC_MOCK_MODE=true EAS_LOCAL_BUILD_ARTIFACTS_DIR=dist eas build -p android --profile preview --local",
    "build:apk:prod": "rm -f .env.local && EAS_LOCAL_BUILD_ARTIFACTS_DIR=dist eas build -p android --profile production-apk --local"
  },
  "dependencies": {
    "@expo-google-fonts/lora": "^0.4.2",
    "@expo-google-fonts/space-grotesk": "^0.4.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@shopify/flash-list": "2.0.2",
    "@supabase/supabase-js": "^2.45.4",
    "expo": "~54.0.25",
    "expo-asset": "~12.0.10",
    "expo-audio": "~1.0.15",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.18",
    "expo-file-system": "~19.0.19",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-localization": "~17.0.7",
    "expo-navigation-bar": "~5.0.9",
    "expo-network": "^8.0.7",
    "expo-notifications": "~0.32.13",
    "expo-router": "~6.0.15",
    "expo-secure-store": "~15.0.7",
    "expo-speech": "~14.0.7",
    "expo-speech-recognition": "^3.0.0",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-updates": "~29.0.13",
    "expo-web-browser": "~15.0.9",
    "moti": "^0.30.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-gifted-charts": "^1.4.64",
    "react-native-purchases": "^9.6.7",
    "react-native-purchases-ui": "^9.6.7",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-template-literals": "^7.27.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "~19.1.0",
    "@vitest/coverage-v8": "^4.0.13",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "expo-mcp": "^0.1.15",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "supabase": "^2.54.11",
    "typescript": "~5.9.2",
    "vite-tsconfig-paths": "^5.1.1",
    "vitest": "^4.0.13"
  },
  "private": true
}
