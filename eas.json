{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "release": {
      "android": { "buildType": "app-bundle" }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "channel": "preview",
      "android": { "buildType": "apk" }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "android": { "buildType": "app-bundle" },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://usuyppgsmmowzizhaoqj.functions.supabase.co/api"
      }
    }
  },
  "submit": {
    "internal": {
      "android": {
        "track": "internal",
        "serviceAccountKeyPath": "./.eas/keys/google-service-account.json"
      }
    },
    "production": {
      "android": {
        "track": "production",
        "serviceAccountKeyPath": "./.eas/keys/google-service-account.json"
      }
    }
  }
}
