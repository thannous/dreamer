{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "release": {
      "android": { "buildType": "app-bundle" },
      "environment": "production"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development"
    },
    "preview": {
      "channel": "preview",
      "android": { "buildType": "apk" },
      "environment": "preview"
    },
    "production-apk": {
      "channel": "production",
      "android": { "buildType": "apk", "credentialsSource": "local" },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://usuyppgsmmowzizhaoqj.functions.supabase.co/api",
        "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "359653779023-6hivlgk9tpgq9g8ng5qpoipjjs780m8h.apps.googleusercontent.com",
        "EXPO_PUBLIC_REFERENCE_IMAGES_ENABLED": "false"
      }
    },
    "e2e-test": {
      "withoutCredentials": true,
      "ios": { "simulator": true },
      "android": { "buildType": "apk" },
      "env": {
        "EXPO_PUBLIC_MOCK_MODE": "true"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "environment": "production",
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "internal": {
      "android": {
        "track": "internal"
      }
    },
    "production": {
      "android": {
        "track": "production"
      }
    }
  }
}
