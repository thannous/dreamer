{
  "expo": {
    "name": "Noctalia",
    "slug": "dream-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dreamapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tanuki75.noctalia"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.tanuki75.noctalia"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow Dream Weaver to access your microphone to record your dreams."
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-asset",
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.359653779023-d3ddevr4qepqa13uqlv1ckd0th6k79gf"
        }
      ],
      "expo-localization",
      "expo-notifications",
      "expo-speech-recognition"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "apiUrl": "https://usuyppgsmmowzizhaoqj.functions.supabase.co/api",
      "supabaseUrl": "https://usuyppgsmmowzizhaoqj.supabase.co",
      "supabaseAnonKey": "sb_publishable_MpacCRXT8NJRcx6q_ww_pw_L_TQWi3n",
      "revenuecatWebKey": "test_zqltcBoDiTWPWmuyXTXTbYkJPrz",
      "eas": {
        "projectId": "942327eb-bbba-4b13-a38a-23bbf64936b8"
      }
    },
    "owner": "tanuki75",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/942327eb-bbba-4b13-a38a-23bbf64936b8"
    }
  }
}
